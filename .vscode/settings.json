{
  "files.eol": "\n",
  "eslint.enable": true,
  "editor.tabSize": 4,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "cSpell.words": [
    "fieldname",
    "maxlength",
    "middlewares",
    "MODELURL",
    "MONGOURI",
    "xrays"
  ]
}